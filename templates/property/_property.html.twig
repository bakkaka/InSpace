<div class="card mb-4">
<div class="card-body">
<h5 class="card-title">
<a href="{{ path('property_show', {'id': property.id })}}">
{{ property.title}}
</a>
</h5>
<p class="card-text">
{{ property.description | nl2br }}
</p>
<p class="card-text">
{{ property.city }} - {{property.postalCode}}
</p>
<div class="text-primary" style="font-size: 2rem; font-weight: blod ">
{{property.price}} $
</div>
</div>
</div>